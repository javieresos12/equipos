<p id="notice"><%= notice %></p>

<h1>Asignaciones</h1>

<table>
  <thead>
    <tr>
      <th>Persona</th>
      <th>Fecha</th>
      <th>Idestadopersonaequipo</th>
      <th>Eliminado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @asignaciones.each do |asignacione| %>
      <tr>
        <td><%= asignacione.persona %></td>
        <td><%= asignacione.fecha %></td>
        <td><%= asignacione.idEstadoPersonaEquipo %></td>
        <td><%= asignacione.eliminado %></td>
        <td><%= link_to 'Show', asignacione %></td>
        <td><%= link_to 'Edit', edit_asignacione_path(asignacione) %></td>
        <td><%= link_to 'Destroy', asignacione, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Asignacione', new_asignacione_path %>
